{
  "name": "happy-tts-gradio",
  "version": "1.0.0",
  "description": "Happy TTS with Gradio-like interface",
  "main": "dist/app.js",
  "scripts": {
    "start": "concurrently \"node dist/app.js\" \"serve -s public -l 3001\"",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --watch src -e ts --exec ts-node src/app.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc && npm run obfuscate",
    "obfuscate": "javascript-obfuscator dist --output dist-obfuscated --config package.json",
    "build:frontend": "cd frontend && npm run build",
    "prod": "npm run build && npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "jest --testPathPattern=authController.test.ts",
    "test:rate-limit": "jest --testPathPattern=rateLimit.test.ts"
  },
  "keywords": [
    "happy",
    "tts"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "@prisma/client": "^6.10.1",
    "@types/bcrypt": "^5.0.2",
    "@types/dompurify": "^3.2.0",
    "@types/express-rate-limit": "^6.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.2",
    "axios": "^1.6.7",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.6",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "happy-tts-gradio": "file:",
    "helmet": "^8.1.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "liquid-glass-react": "^1.1.1",
    "morgan": "^1.10.0",
    "openai": "^5.5.1",
    "qrcode": "^1.5.4",
    "serve": "^14.2.1",
    "speakeasy": "^2.0.0",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^24.0.3",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.1.2",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^30.0.2",
    "nodemon": "^3.1.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}

{
  "name": "happy-tts-gradio",
  "version": "1.0.0",
  "description": "Happy TTS with Gradio-like interface",
  "main": "dist/app.js",
  "scripts": {
    "start": "concurrently \"node dist/app.js\" \"serve -s public -l 3001\"",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --watch src -e ts --exec ts-node src/app.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "prod": "npm run build && npm start"
  },
  "keywords": [
    "happy",
    "tts"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "happy-tts-gradio": "file:",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "openai": "^4.0.0",
    "serve": "^14.2.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^24.0.3",
    "concurrently": "^9.1.2",
    "jest": "^30.0.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
